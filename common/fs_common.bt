#ifndef FS_COMMON_BT
#define FS_COMMON_BT

struct StreamChunk
{
	char Type[4];
	uint Size;
	double Time;

	if (Type != "END " && Type != " DNE")
		FS_CHUNK_PAYLOAD Payload;
};

while (true)
{
	StreamChunk Chunk;

	// END signifies last  chunk
	if (Chunk.Type == "END " || Chunk.Type == " DNE")
		break;

	FSeek(startof(Chunk) + Chunk.Size);
}
#endif